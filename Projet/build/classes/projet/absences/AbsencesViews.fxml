<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="725.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projet.absences.AbsencesViewsController">
   <children>
      <TableView fx:id="table" layoutY="-1.0" prefHeight="351.0" prefWidth="725.0">
        <columns>
          <TableColumn fx:id="col_id" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="col_date" prefWidth="75.0" text="Date_Abs" />
            <TableColumn fx:id="col_heure_debut" prefWidth="75.0" text="H_Debut" />
            <TableColumn fx:id="col_heure_fin" prefWidth="75.0" text="H_Fin" />
            <TableColumn fx:id="col_enseignantid" prefWidth="121.0" text="Enseignant_ID" />
            <TableColumn fx:id="col_eleveid" prefWidth="121.0" text="Eleve_ID" />
            <TableColumn fx:id="col_justification" prefWidth="101.0" text="Justification" />
            <TableColumn fx:id="col_etat" prefWidth="81.0" text="Etat" />
        </columns>
      </TableView>
      <Button fx:id="ajouter" layoutX="6.0" layoutY="355.0" mnemonicParsing="false" onAction="#ajouterAbsence" prefHeight="31.0" prefWidth="100.0" style="-fx-background-color: green;" text="Ajouter" textFill="WHITE" />
      <Button fx:id="modifier" layoutX="113.0" layoutY="355.0" mnemonicParsing="false" onAction="#ModifierAbsence" prefHeight="31.0" prefWidth="100.0" style="-fx-background-color: orange;" text="Modifier" />
      <Button fx:id="supprimer" layoutX="223.0" layoutY="355.0" mnemonicParsing="false" onAction="#SupprimerAbsence" prefHeight="31.0" prefWidth="120.0" style="-fx-background-color: red;" text="Supprimer" textFill="WHITE" />
   </children>
</AnchorPane>
