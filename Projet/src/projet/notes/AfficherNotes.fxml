<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="728.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projet.notes.NotesViewsController">
   <children>
      <TableView fx:id="table" layoutX="-1.0" layoutY="-1.0" prefHeight="346.0" prefWidth="735.0">
        <columns>
          <TableColumn fx:id="col_Id" prefWidth="102.0" text="ID" />
          <TableColumn fx:id="col_type" prefWidth="87.0" text="Type" />
            <TableColumn fx:id="col_trimestre" prefWidth="103.0" text="Trimestre" />
            <TableColumn fx:id="col_enseignantid" prefWidth="106.0" text="Enseignant_ID" />
            <TableColumn fx:id="col_eleveid" prefWidth="122.0" text="Eleve_ID" />
            <TableColumn fx:id="col_matiere" prefWidth="109.0" text="Matiere" />
            <TableColumn fx:id="col_valeur" prefWidth="105.0" text="Valeur" />
        </columns>
      </TableView>
      <Button fx:id="modifier" layoutX="102.0" layoutY="355.0" mnemonicParsing="false" onAction="#envoyermodifierNote" style="-fx-background-color: orange;" text="Modifier" textFill="#282626" />
      <Button fx:id="supprimer" layoutX="192.0" layoutY="355.0" mnemonicParsing="false" onAction="#SupprimerNote" style="-fx-background-color: red;" text="Supprimer" textFill="WHITE" />
