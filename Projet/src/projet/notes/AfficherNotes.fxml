      <Button fx:id="boutton_moyenne" layoutX="290.0" layoutY="355.0" mnemonicParsing="false" onAction="#envoyermoyenne" prefHeight="31.0" prefWidth="97.0" style="-fx-background-color: blue;" text="Moyenne" textFill="WHITE" />
   </children>
</AnchorPane>
